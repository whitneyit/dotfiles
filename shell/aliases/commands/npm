#!/usr/bin/env bash

alias  ni="npm install"
alias nig="npm install --global"
alias nis="npm install --save"
alias nid="npm install --save-dev"
alias  nl="npm list --depth=0"
alias nlg="npm list --depth=0 --global"
alias  np="npm prune"
alias  ns="npm start"
alias  nt="npm test"
alias  nv="npm --version && node --version"

